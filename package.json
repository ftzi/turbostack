{
	"name": "my-project",
	"packageManager": "bun@1.3.3",
	"private": true,
	"scripts": {
		"build": "turbo build",
		"dev": "bun i && turbo dev tsw",
		"lint": "bunx biome check --write --unsafe . --error-on-warnings",
		"ts": "turbo ts",
		"ok": "turbo ts && bun lint",
		"knip": "knip",
		"tsw": "turbo tsw",
		"db:studio": "bun run --cwd packages/api db:studio",
		"db:generate": "bun run --cwd packages/api db:generate",
		"db:migrate": "bun run --cwd packages/api db:migrate",
		"env": "bunx vercel env pull .env"
	},
	"devDependencies": {
		"@types/bun": "1.3.3",
		"@types/node": "24.10.1",
		"@workspace/typescript-config": "workspace:*",
		"knip": "5.70.1",
		"turbo": "2.6.1",
		"typescript": "5.9.3",
		"@biomejs/biome": "2.3.7"
	},
	"workspaces": {
		"packages": [
			"apps/*",
			"packages/*"
		],
		"catalog": {
			"react": "19.2.0",
			"react-dom": "19.2.0",
			"@types/react": "19.2.2",
			"@types/react-dom": "19.2.2",
			"typescript": "5.9.3",
			"lucide-react": "0.552.0",
			"zod": "4.1.12",
			"@orpc/client": "1.11.3",
			"@orpc/contract": "1.11.3",
			"@orpc/server": "1.11.3",
			"next": "16.0.1",
			"server-only": "0.0.1"
		}
	},
	"engines": {
		"node": ">=20"
	}
}
