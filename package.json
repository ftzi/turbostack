{
  "name": "my-project",
  "devDependencies": {
    "@types/bun": "1.3.3",
    "@types/node": "24.10.1",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "drizzle-kit": "0.31.7",
    "knip": "5.70.1",
    "turbo": "2.6.1",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "bun@1.3.3",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "bun i && turbo dev tsw",
    "lint": "turbo lint",
    "ts": "turbo ts",
    "ok": "turbo ts lint",
    "knip": "knip",
    "tsw": "turbo tsw",
    "format:dry:TODO": "biome check --unsafe && next lint",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "env": "bunx vercel env pull .env"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "react": "19.2.0",
      "react-dom": "19.2.0",
      "@types/react": "19.2.2",
      "@types/react-dom": "19.2.2",
      "typescript": "5.9.3",
      "lucide-react": "0.552.0",
      "@biomejs/biome": "2.3.4",
      "zod": "4.1.12",
      "@orpc/client": "1.11.3",
      "@orpc/contract": "1.11.3",
      "@orpc/server": "1.11.3"
    }
  }
}
